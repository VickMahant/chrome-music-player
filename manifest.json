{
  "manifest_version": 2,

  "name": "Chrome Music Player",
  "description": "Plays songs from your bookmarks",
  "version": "1.0",

  "options_page": "options.html",

  "icons": {
    "48": "icon.png"
  },

  "permissions": [
    "bookmarks",
    "tabs", 
    "<all_urls>"
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}